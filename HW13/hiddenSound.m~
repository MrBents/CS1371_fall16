function out = hiddenSound( img )

img = imread(img);
left = img(:,:,1);
right = img(:,:,2);
scale = img(:,:,3);

mask = scale == 0;

left = doubleleft(~mask);
right = right(~mask);
scale = scale(~mask);

left = (double(left - 128)./128).*(scale./256);
right = (double(right - 128)./128).*(scale./256);

left = reshape(left, [], 1);
right = reshape(right, [], 1);

out = [left, right];

end

